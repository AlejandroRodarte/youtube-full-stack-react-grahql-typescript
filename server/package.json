{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && npm run env-cmd:prod -- node dist/index.js",
    "env-cmd:prod": "env-cmd -f ./config/.env.production",
    "env-cmd:dev": "env-cmd -f ./config/.env.development",
    "start:dev": "env-cmd -f ./config/.env.development ts-node-dev src/index.ts",
    "migration:dev:create:initial": "npm run env-cmd:dev -- mikro-orm migration:create --initial",
    "migration:dev:create": "npm run env-cmd:dev -- mikro-orm migration:create",
    "migration:dev:up": "npm run env-cmd:dev -- mikro-orm migration:up"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.5.7",
    "@mikro-orm/core": "^4.5.7",
    "@mikro-orm/migrations": "^4.5.7",
    "@mikro-orm/postgresql": "^4.5.7",
    "apollo-server-express": "^3.1.2",
    "argon2": "^0.28.2",
    "class-validator": "^0.13.1",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "pg": "^8.7.1",
    "type-graphql": "^1.1.1"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/db/orm/mikro-orm.config.ts",
      "./dist/db/orm/mikro-orm.config.js"
    ]
  }
}
