{
  "name": "server",
  "version": "1.0.0",
  "description": "lireddit server",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "env-cmd:prod": "env-cmd -f ./config/.env.production",
    "env-cmd:dev": "env-cmd -f ./config/.env.development",
    "start:dev": "env-cmd -f ./config/.env.development ts-node-dev --transpile-only src/index.ts",
    "start:prod": "tsc && npm run env-cmd:prod -- node dist/index.js",
    "postinstall": "npm run build",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/db/orm/typeorm/config.ts",
    "migration:generate": "npm run env-cmd:dev -- npm run typeorm:cli migration:generate -- -n"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/connect-redis": "^0.0.17",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^16.6.1",
    "@types/nodemailer": "^6.4.4",
    "@types/redis": "^2.8.31",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/ioredis": "^4.27.1",
    "@types/uuid": "^8.3.1",
    "apollo-server-express": "^3.1.2",
    "argon2": "^0.28.2",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "graphql": "^15.5.1",
    "ioredis": "^4.27.8",
    "joi": "^17.4.2",
    "nodemailer": "^6.6.3",
    "pg": "^8.7.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2"
  }
}
